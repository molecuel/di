    "gulp-sourcemaps": "^2.2.2",
    "gulp-tsconfig-files": "^0.2.5",
    "gulp-tslint": "^4.3.2",
    "gulp-typedoc": "^2.0.1",
    "gulp-typescript": "^3.1.3",
    "gulp-util": "^3.0.7",
    "gulpclass": "0.0.8",
